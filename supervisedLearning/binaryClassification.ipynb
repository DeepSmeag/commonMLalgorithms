   "source": [
    "def gradient_descent_binary(X, y, epochs = 1000, eta = 0.01, dim = 1):\n",
    "    theta = np.random.randn(dim+1)\n",
    "    costs =[]\n",
    "    for ep in range(epochs):\n",
    "        yhat = sigmoid(X.dot(theta))\n",
    "        cost = cross_entropy(y, yhat)\n",
    "        costs.append(cost)\n",
    "        gradient = X.T.dot(cross_entropy_derivative(y, yhat))\n",
    "        theta -= eta*gradient\n",
    "    return theta, costs\n",
    "\n",
    "theta , costs = gradient_descent_binary(X,y,10000,0.01)\n",
    "    \n",
    "plt.plot(costs)\n",
    "\n"
   ]
